  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "5.*",
    "illuminate/filesystem": "5.*",
    "pagarme/pagarme-php": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*"
  },
  "autoload": {
    "psr-4": {
      "Deskti\\Pagarme\\": "src/"
    }
